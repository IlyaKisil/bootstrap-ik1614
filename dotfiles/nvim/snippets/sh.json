{
  "user": {
    "prefix": "user",
    "body": "${VIM:\\$USER}"
  },
  "here": {
    "prefix": "here",
    "body": "HERE=\"\\$( cd \"\\$( dirname \"\\${BASH_SOURCE[0]}\" )\" >/dev/null 2>&1 && pwd )\"",
    "description": "Get the full path to location of current script"
  },
  "single constant": {
    "prefix": "co",
    "body": "const ${1:name} = ${2:value}",
    "description": "Snippet for a constant"
  }
}
